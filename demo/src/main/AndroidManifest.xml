<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guoxiaoxing.java.demo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".annotation.AnnotationActivity" />
        <activity android:name=".grammar.GrammarActivity" />
        <activity android:name=".io.OutputStreamActivity" />
        <activity android:name=".object.ObjectActivity" />
        <activity android:name=".collection.CollectionActivity" />
        <activity android:name=".concurrence.ConcurrenceActivity" />
        <activity android:name=".jvm.JvmActivity"></activity>
    </application>

</manifest>